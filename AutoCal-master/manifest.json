  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://kit.fontawesome.com https://stackpath.bootstrapcdn.com https://code.jquery.com https://cdnjs.cloudflare.com https://apis.google.com/; object-src 'self'"
